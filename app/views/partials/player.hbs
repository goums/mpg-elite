{{#if (gt 12 number)}}
<li class="player {{#if (eq definitiveRating false)}}{{#if rating}}index__liveRating___3fl2_{{/if}}{{/if}}"
  data-position="{{number}}">
  {{else}}
  <span
    class="index__substitute___H6R8K {{#if (eq definitiveRating false)}}{{#if rating}}index__liveRating___3fl2_{{/if}}{{/if}}"
    data-position="{{number}}">
    {{/if}}

    <span class="team-graphic" role="figure">
      <span class="graphic-overlay graphic-tooltip" data-tippy="" data-original-title="{{name}}">
        {{#if jerseyUrl }}
          <img src="{{jerseyUrl}}" data-teamid="{{teamid}}" class="team-jersey jersey-{{teamid}}">
        {{else}}
        <img
          src="data:image/png;base64,"
          data-teamid="0" class="team-jersey jersey-0">
        {{/if}}
        <span class="goals">
          {{#if goals}}
          {{#each (loop goals.goal)}}
          <span class="index__ball___39Bld index__root___2XTpz jss6 goal">{{> graphics/goal}}</span>
          {{/each}}
          {{#if goals.mpg}}
          <span class="index__ball___39Bld index__mpg___uUgmt index__root___2XTpz jss6 goal">{{>
            graphics/goal}}</span>
          {{/if}}
          {{#if goals.own_goal}}
          {{#each (loop goals.own_goal)}}
          <span class="index__ball___39Bld index__csc___3wT7u index__root___2XTpz jss6 goal">{{>graphics/goal}}</span>
          {{/each}}
          {{/if}}
          {{/if}}
        </span>
        <span class="status {{#if rating}}rate{{/if}}">
          {{#if rating}}
          {{rating}}
          {{#if bonus}}
          <span class="rate"
            style="top: 1px;right: -15px;width: 14px;height: 12px;line-height: 13px;padding: 0 1px 0px 0;border-radius: 0;border-left: 0;font-size: 9px;border-top-right-radius: 5px;border-bottom-right-radius: 5px;">
            +{{#if (eq bonus 0.5)}}.5{{else}}{{bonus}}{{/if}}
          </span>
          {{/if}}
          {{else}}
          {{#if (eq starter 2)}}
          <span class="index__holder___3xsXV index__sub___2EbIE"><i class="fa fa-street-view"></i></span>
          {{else}}
          {{#if (eq starter 1)}}
          <span class="index__holder___3xsXV"><i class="fa fa-street-view"></i></span>
          {{else}}
          {{#if (eq starter -1)}}
          <span class="index__holder___3xsXV index__notHolder___3BT_J"><i class="fa fa-street-view"></i></span>
          {{/if}}
          {{/if}}
          {{/if}}
          {{/if}}
        </span>
        {{#if captain}}
        <span class="status captain">C</span>
        {{/if}}
      </span>
      <span class="label">{{name}}</span>
    </span>

    {{#if (gt 12 number)}}
</li>
{{else}}
</span>
{{/if}}